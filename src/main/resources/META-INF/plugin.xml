<idea-plugin>
	<id>org.antlr.jetbrains.sample</id>
	<name>GDScript</name>
	<version>0.1</version>
	<vendor>test</vendor>
	<idea-version since-build="192"/> <!-- version 2019.2 -->
	<depends>com.intellij.modules.lang</depends>
	<extensions defaultExtensionNs="com.intellij">
		<lang.syntaxHighlighterFactory language="GDScript" implementationClass="gdscript.highlight.SyntaxHighlighterFactory"/>
		<lang.parserDefinition language="GDScript" implementationClass="gdscript.parser.ParserDefinition"/>
		<lang.commenter language="GDScript" implementationClass="gdscript.commenter.Commenter"/>
		<completion.contributor language="GDScript" implementationClass="gdscript.completion.BuiltInFunctionsCompletionContributor"/>
		<completion.contributor language="GDScript" implementationClass="gdscript.completion.KeywordCompletionContributor"/>
		<completion.contributor language="GDScript" implementationClass="gdscript.completion.ColorCompletionContributor"/>
		<fileTypeFactory implementation="gdscript.FileTypeFactory"/>
		<colorSettingsPage implementation="gdscript.colors.ColorSettingsPage"/>
	</extensions>
</idea-plugin>
