<idea-plugin>
	<id>gdscript</id>
	<name>Godot GDScript</name>
	<version>0.18</version>
	<vendor>Jakub BÅ‚ach</vendor>
	<description><![CDATA[
		<p>Adds support for the Godot GDScript language.
		<p>(<a href="https://github.com/exigow/intellij-gdscript/issues/new">Report Issue</a>)
    ]]></description>
	<depends>com.intellij.modules.lang</depends>
	<extensions defaultExtensionNs="com.intellij">
		<lang.parserDefinition implementationClass="gdscript.parser.ScriptParserDefinition" language="GDScript"/>
		<lang.parserDefinition implementationClass="gdscript.parser.ResourceParserDefinition" language="GDScript resource"/>
		<lang.syntaxHighlighterFactory implementationClass="gdscript.highlight.factories.ScriptHighlighterFactory" language="GDScript"/>
		<lang.syntaxHighlighterFactory implementationClass="gdscript.highlight.factories.ResourceHighlighterFactory" language="GDScript resource"/>
		<completion.contributor implementationClass="gdscript.completion.StaticCompletionContributor" language="GDScript"/>
		<completion.contributor implementationClass="gdscript.completion.TypeCompletionContributor" language="GDScript"/>
		<completion.contributor implementationClass="gdscript.completion.ValueCompletionContributor" language="GDScript"/>
		<completion.contributor implementationClass="gdscript.completion.KeywordCompletionContributor" language="GDScript"/>
		<completion.contributor implementationClass="gdscript.completion.DataCompletionContributor" language="GDScript"/>
		<annotator implementationClass="gdscript.annotation.ClassAnnotator" language="GDScript"/>
		<annotator implementationClass="gdscript.annotation.InstanceFieldAnnotator" language="GDScript"/>
		<annotator implementationClass="gdscript.annotation.MethodAnnotator" language="GDScript"/>
		<annotator implementationClass="gdscript.annotation.UpperCaseConstantAnnotator" language="GDScript"/>
		<lang.braceMatcher implementationClass="gdscript.typing.ScriptBraceMatcher" language="GDScript"/>
		<lang.quoteHandler implementationClass="gdscript.typing.ScriptQuoteHandler" language="GDScript"/>
		<lang.commenter implementationClass="gdscript.commenter.ScriptCommenter" language="GDScript"/>
		<lang.commenter implementationClass="gdscript.commenter.ResourceCommenter" language="GDScript resource"/>
		<colorSettingsPage implementation="gdscript.options.ColorSettingsPage"/>
		<fileType implementationClass="gdscript.file.ScriptType" extensions="gd" name="Godot GDScript file" language="GDScript"/>
		<fileType implementationClass="gdscript.file.ResourceType" extensions="godot" name="Project file" language="GDScript resource"/>
		<fileType implementationClass="gdscript.file.ResourceType" extensions="import" name="Import file" language="GDScript resource"/>
		<fileType implementationClass="gdscript.file.ResourceType" extensions="cfg" name="Configuration file" language="GDScript resource"/>
		<fileType implementationClass="gdscript.file.ResourceType" extensions="tres" name="Text resource file" language="GDScript resource"/>
		<fileType implementationClass="gdscript.file.ResourceType" extensions="tscn" name="Text scene file" language="GDScript resource"/>
	</extensions>
</idea-plugin>
