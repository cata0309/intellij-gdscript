<idea-plugin>
	<id>org.antlr.jetbrains.sample</id>
	<name>GDScript</name>
	<version>0.1</version>
	<vendor>test</vendor>
	<idea-version since-build="183"/>
	<depends>com.intellij.modules.lang</depends>
	<extensions defaultExtensionNs="com.intellij">
		<lang.syntaxHighlighterFactory implementationClass="plugin.highlighting.GDScriptHighlighterFactory" language="GDScript"/>
		<lang.parserDefinition implementationClass="plugin.parser.GDScriptParserDefinition" language="GDScript"/>
		<lang.commenter implementationClass="plugin.GDScriptCommenter" language="GDScript"/>
		<completion.contributor implementationClass="plugin.completion.DocumentationCompletionContributor" language="GDScript"/>
		<completion.contributor implementationClass="plugin.completion.KeywordCompletionContributor" language="GDScript"/>
		<completion.contributor implementationClass="plugin.completion.ColorCompletionContributor" language="GDScript"/>
		<fileTypeFactory implementation="plugin.GDScriptFileTypeFactory"/>
		<colorSettingsPage implementation="plugin.highlighting.GDScriptColorSettingsPage"/>
	</extensions>
</idea-plugin>
