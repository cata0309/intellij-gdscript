<idea-plugin>
	<name>GDScript</name>
	<version>0.1</version>
	<vendor></vendor>
	<idea-version since-build="133"/>
	<depends>com.intellij.modules.lang</depends>
	<project-components>
		<component>
			<implementation-class>plugin.SamplePluginController</implementation-class>
		</component>
	</project-components>
	<extensions defaultExtensionNs="com.intellij">
		<fileTypeFactory implementation="plugin.GDScriptFileTypeFactory"/>
		<lang.syntaxHighlighterFactory key="Sample" implementationClass="plugin.GDScriptSyntaxHighlighterFactory"/>
		<lang.parserDefinition language="Sample" implementationClass="plugin.GDScriptParserDefinition"/>
		<lang.ast.factory language="Sample" implementationClass="plugin.GDScriptASTFactory"/>
		<lang.psiStructureViewFactory language="Sample" implementationClass="plugin.structview.SampleStructureViewFactory"/>
		<colorSettingsPage implementation="plugin.SampleColorSettingsPage"/>
		<externalAnnotator language="Sample" implementationClass="plugin.GDScriptExternalAnnotator"/>
		<lang.findUsagesProvider language="Sample" implementationClass="plugin.GDScriptFindUsagesProvider"/>
	</extensions>
</idea-plugin>
