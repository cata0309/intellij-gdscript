<idea-plugin>
	<id>gdscript</id>
	<name>GDScript</name>
	<version>0.1</version>
	<vendor>Jakub BÅ‚ach</vendor>
	<idea-version since-build="192"/> <!-- version 2019.2 -->
	<depends>com.intellij.modules.lang</depends>
	<extensions defaultExtensionNs="com.intellij">
		<!-- config files -->
		<lang.commenter implementationClass="gdscript.ConfigCommenter" language="Config"/>
		<fileType implementationClass="gdscript.ConfigFileType" extensions="cfg" name="Godot CFG file" language="Config"/>
		<fileType implementationClass="gdscript.ConfigFileType" extensions="import" name="Godot IMPORT file" language="Config"/>
		<fileType implementationClass="gdscript.ConfigFileType" extensions="tres" name="Godot TRES file" language="Config"/>
		<fileType implementationClass="gdscript.ConfigFileType" extensions="godot" name="Godot PROJECT file" language="Config"/>
		<lang.syntaxHighlighterFactory implementationClass="gdscript.ConfigHighlighterFactory" language="Config"/>
		<lang.parserDefinition implementationClass="gdscript.ConfigParserDefinition" language="Config"/>
		<!-- gdscript files -->
		<colorSettingsPage implementation="gdscript.ScriptColorSettingsPage"/>
		<lang.commenter implementationClass="gdscript.ScriptCommenter" language="GDScript"/>
		<completion.contributor implementationClass="gdscript.ScriptCompletionContributor" language="GDScript"/>
		<fileType implementationClass="gdscript.ScriptFileType" extensions="gd" name="Godot GDScript file" language="GDScript"/>
		<lang.syntaxHighlighterFactory implementationClass="gdscript.ScriptHighlighterFactory" language="GDScript"/>
		<lang.parserDefinition implementationClass="gdscript.ScriptParserDefinition" language="GDScript"/>
	</extensions>
</idea-plugin>
